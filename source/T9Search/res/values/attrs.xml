<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="com.baidu.mobads.AdView">
        <attr name="adSize" format="integer" />
        <attr name="adId" format="string" />
    </declare-styleable>
    <declare-styleable name="SettingItemView">
        <attr name="titleText" format="string" />
        <attr name="subTitleText" format="string" />
        <attr name="settingType">
            <enum name="type_click" value="1"></enum>
            <enum name="type_check" value="2"></enum>
            <enum name="type_title" value="3"></enum>
        </attr>
    </declare-styleable>

    <declare-styleable name="KeyboardButtonView">
        <attr name="mainText" format="string" />
        <attr name="subText" format="string" />
        <attr name="buttonIcon" format="reference" />
    </declare-styleable>
    
    <item name="tag_index" type="id"></item>
    <item name="tag_appinfo" type="id"></item>

</resources>